html,
body
{
	overflow-x: hidden;
}
body
{
	background: #fff url(/local/templates/ilab_it_shop/ilab/img/fon/background.png) repeat-y scroll center top;
}
body.i_personal_page
{
	background-color: #f6f6f6;
}
body .i_wrapper
{
	background: #fff !important;
	box-shadow: 0 2px 3px rgba(0,0,0,0.5);
}
.i_wrapper input[type='text'],
.i_wrapper input[type='email'],
.i_wrapper input[type='password'],
.i_wrapper select,
.i_wrapper textarea
{
	font-size: 14px;
	color: #131b20;
	border: 1px solid #e9e9e9;
}
.i_wrapper
{
	background: 0 none !important;
}
input
{
	outline:none;
}
.i_co_numb
{
	font-weight: bold;
}
/* ---------------------------------------------- i_tblock */
.i_tblock
{
	background: #4d3b3b;
}
.i_log_sep
{
	border-left-color: #fff;
}
@media screen and (min-width: 640px)
{
	.i_tblock .i_tm_a
	{
		color: #fff;
	}
	.i_login a
	{
		color: #fff;
	}
	.i_tm_a:hover,
	.i_tm_a.i_more.i_tm_hover
	{
		background-color: #fff;
		color: #4d3b3b;
	}
	.i_tm_a.i_more:after
	{
		background-image: url(/local/templates/ilab_it_shop/ilab/img/svg/arrows/down_white.svg);
	}
	.i_tm_a.i_more:hover:after,
	.i_tm_a.i_more.i_tm_hover:after
	{
		background-image: url(/local/templates/ilab_it_shop/ilab/img/svg/arrows/down_color.svg);
	}
	.i_sub,
	.i_mo
	{
		background-color: #fff;
	}
	.i_sub_a,
	.i_mo_a
	{
		color: #131b20;
	}
	.i_sub_a:hover,
	.i_sub_selected,
	.i_mo_a:hover
	{
		background-color: #4d3b3b;
		color: #fff;
	}
	.jq_sub_a:before
	{
		background-image: url(/local/templates/ilab_it_shop/ilab/img/svg/arrows/left_black.svg);
	}
	.jq_sub_a:hover:before,
	.jq_sub_a.i_sub_selected:before
	{
		background-image: url(/local/templates/ilab_it_shop/ilab/img/svg/arrows/left_white.svg);
	}
}
@media screen and (max-width: 639px)
{
	.i_tm_item:not(:first-child)
	{
		border-top: 1px solid #fff;
	}
	.i_tm_a,
	.i_sub_a
	{
		color: #131b20;
		font-size: 16px;
	}
	.i_tm_a
	{
		background-color: #eff2f4;
	}
	.i_sub_a
	{
		background-color: #fff;
	}
	.i_tm_a:hover,
	.i_tm_a.i_more.i_tm_hover
	{
		background-color: #4d3b3b;
		color: #fff;
	}
	.i_sub_a_text:before
	{
		background-color: #4d3b3b;
	}
	.i_sub_a:hover
	{
		background-color: #4d3b3b;
		color: #fff;
	}
	.i_sub_a:hover .i_sub_a_text:before
	{
		background-color: #fff;
	}
	.i_tblock .i_tm_tt span
	{
		background: #fff;
	}
	.i_tm_a.i_more .i_tm_ar {
		background-image: url(/local/templates/ilab_it_shop/ilab/img/svg/arrows/right_grow.svg);
	}
}
/* ---------------------------------------------- .i_header */
.i_header
{
	position: relative;
}
.i_hcontact
{
	color: #000;
}
.i_hlogo
{
	padding: 20px 0;
}
.i_line_header_icons .i_fmap
{
	color: #4d3b3b;
}
.i_line_header_v3 #title-search-input
{
	background-color: #fff;
