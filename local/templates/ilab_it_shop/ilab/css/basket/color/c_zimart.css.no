	border-bottom: 0 none;
	color: white;
}
.i_or_person_type .label label { cursor: pointer }

.i_or_person_type .label:first-child {
	border-radius: 5px 0 0 5px;
}
.i_or_person_type .label:last-child {
	border-radius: 0 5px 5px 0;
