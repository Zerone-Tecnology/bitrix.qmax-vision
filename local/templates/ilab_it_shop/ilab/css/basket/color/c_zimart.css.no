body{
	font-family: PTSans-Regular;
	color:#2a2d2f;
}

h1, h2, h3, h4, h5, h6{
	font-weight: normal;
	font-family: PTSans-Bold;
}

b, strong{
	font-weight:normal;
	font-family: PTSans-Bold;
}

a, a:hover { color: #5c8299 }
/* ---------------------------------------------------------------------------------------------------- Image */
.i_fphone_tele, .i_fphone_mobi, .i_modal_close,
.i_ba_fpay, .i_ba_dmet, .i_hback
{ background-image: url(/local/templates/ilab_it_shop/ilab/img/color/i_zimart.png) }
/* ---------------------------------------------------------------------------------------------------- Button */
.i_wrapper input[type='submit'],
.i_wrapper input[type='button'],
.i_but_ac,
.i_but_in { color: #FFF }
.i_wrapper input[type='submit'],
.i_wrapper input[type='button'],
.i_but_ac {
	border-color: #2a2d2f;
	background: #2a2d2f;
	text-transform: uppercase;
	font-weight: normal;
}
.i_but_in {
	background: #606060;
	background: url(data:image/svg+xml;base64,);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#606060), color-stop(4%,#7d7d7d), color-stop(11%,#6b6b6b), color-stop(14%,#676767), color-stop(21%,#656565), color-stop(46%,#575757), color-stop(50%,#424242), color-stop(68%,#363636), color-stop(86%,#2d2d2d), color-stop(89%,#2f2f2f), color-stop(93%,#373737), color-stop(100%,#4e4e4e));
	background: -webkit-linear-gradient(top,  #606060 0%,#7d7d7d 4%,#6b6b6b 11%,#676767 14%,#656565 21%,#575757 46%,#424242 50%,#363636 68%,#2d2d2d 86%,#2f2f2f 89%,#373737 93%,#4e4e4e 100%);
	background: -o-linear-gradient(top,  #606060 0%,#7d7d7d 4%,#6b6b6b 11%,#676767 14%,#656565 21%,#575757 46%,#424242 50%,#363636 68%,#2d2d2d 86%,#2f2f2f 89%,#373737 93%,#4e4e4e 100%);
	background: -ms-linear-gradient(top,  #606060 0%,#7d7d7d 4%,#6b6b6b 11%,#676767 14%,#656565 21%,#575757 46%,#424242 50%,#363636 68%,#2d2d2d 86%,#2f2f2f 89%,#373737 93%,#4e4e4e 100%);
	background: linear-gradient(to bottom,  #606060 0%,#7d7d7d 4%,#6b6b6b 11%,#676767 14%,#656565 21%,#575757 46%,#424242 50%,#363636 68%,#2d2d2d 86%,#2f2f2f 89%,#373737 93%,#4e4e4e 100%);
}
.i_but_ou {
	background-color: #EFF2F4;
	border-color: #DFE0E8;
}
/* ---------------------------------------------------------------------------------------------------- Buy [Section] */
.i_buy_buttom, .i_buy_bought {
	font-weight: normal;
	text-transform: uppercase;
	color: #FFF }
.i_buy_buttom:hover,
.i_buy_bought:hover { color: #FFF }
.i_buy_buttom {
	border-color: #2a2d2f;
	background: #2a2d2f;
}
.i_buy_bought,
.icard_buy_bought {
	border-color: #8f5636;
	background-color: #ffecd8;
}
/* ---------------------------------------------------------------------------------------------------- Header */
.i_hback {
	background-color: #fff;
	border-color: #e4e4e4;
	color: #5c8299;
	padding: 10px 25px 10px 40px;
	background-position: 17px -747px;
}
/* ---------------------------------------------------------------------------------------------------- Footer */
.i_footer {
	border-color: #e4e4e4;
}
.i_footer .i_fphone_tele span,
.i_footer .i_fphone_mobi span,
.i_footer .i_femail span,
.i_footer .i_femail a{
	color:#2a2d2f!important;
}
/* ---------------------------------------------------------------------------------------------------- Modal window */
.i_modal_tit {
	color: #FFF;

	border-color: #2a2d2f;
	text-shadow: 0 -1px 1px rgba(0, 0, 0, .37);

	background: #2a2d2f;
}
.i_modal { box-shadow: none }
/* ---------------------------------------------------------------------------------------------------- SKU */
.i_sku { border-color: #e4e4e4 }
.i_sku li {
	background-color: white;
	border-color: #e4e4e4;
}
.i_sku li.i_sku_color { box-shadow: 0 0 0 2px #FFF inset }
/* ---------------------------------------------------------------------------------------------------- Оформить заказ */
.place_order_div { background-color: #fff;
border:1px solid #e4e4e4}
.bx_order_make .place_order_div {background: url(/local/templates/ilab_it_shop/ilab/img/color/zimart/i_newlist.png);
border-color: #fff;}
