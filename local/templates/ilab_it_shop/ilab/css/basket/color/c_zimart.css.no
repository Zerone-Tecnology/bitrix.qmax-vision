.i_ba_count {}
.i_ba_count input {
	border-radius: 0!important;
	height: 35px;
}
.i_ba_plus,
.i_ba_minus {
	background-color: #eff2f4;
	background-position: center center;
	background-repeat: no-repeat;
	cursor: pointer;
	height: 35px;
	width: 35px;
	display: inline-block;
	vertical-align: middle;
}
.i_ba_change_mp {
	text-align: center;
	border-left: 0 none!important;
	border-right: 0 none!important;
	border-color: #edf0f6!important;
}
.i_ba_change_mp_d {
	vertical-align: middle;
	display: inline-block;
	position: relative;
	width:34px;
}
.i_ba_change_mp_d:after {
	content: attr(cssmeasure);
	position: absolute;
	top:8px;right:5px;
	display: none;
}
.i_ba_plus {
	background: #eff2f4 url(data:image/png;base64,) no-repeat center center;
	border-radius: 0 5px 5px 0;
}
.i_ba_minus {
	background: #eff2f4 url(data:image/png;base64,) no-repeat center center;
	border-radius: 5px 0 0 5px;
}

.custom .i_ba_count input{
	border-color: #eff2f4;
	font-family: PTSans-Bold;
}

