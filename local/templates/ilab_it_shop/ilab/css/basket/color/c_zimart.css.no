.i_or_person_type .label {
	background-color: #fff;
	cursor: pointer;
}
.i_or_person_type .label span {
	border: 0 none;
	color: #5c8299;
	font-weight: normal;
}

.label:not(.i_or_checked){
	background: url(/local/templates/ilab_it_shop/ilab/img/color/zimart/i_newlist.png);
}
.label:not(.i_or_checked) span{
	border-bottom: 1px dashed;
}

