#basket_items {
	position: relative;
}
#basket_items > tbody:after {
    border-bottom: 1px solid #e1dfe8;
    border-left: 1px solid #e1dfe8;
    border-radius: 0 0 5px 5px;
    border-right: 1px solid #e1dfe8;
    bottom: 0;
    content: "";
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    z-index: -1;
}
