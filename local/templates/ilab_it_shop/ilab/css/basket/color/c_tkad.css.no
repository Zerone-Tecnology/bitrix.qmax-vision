/* ---------------------------------------------------------------------------------------------------------- */
.i_bs_title {
	border-color: #e4eaef;
	background-color: #e4eaef;
}
.i_bs_title::before {
	border-color: #1f397e;
	background:  #1f397e url(data:image/png;base64,) no-repeat 8px 12px;
}
.i_bs_title::after {
	border-left: 10px solid #1f397e;
	border-top: 10px solid transparent;
	border-bottom: 10px solid transparent;
	width: 0;
	height: 0;
	top: 50%;
	margin-top: -10px;
}
.bx_ordercart .bx_ordercart_order_table_container {
	border-color: #eff2f4;
	border-top-color: #e4eaef;
}
.bx_ordercart .bx_sort_container a {
	background: #fff;
	border-color: #e4e4e4;
}
.bx_ordercart .bx_sort_container a.current {
	background: #1f397d;
}
.basket_items_row div {
	background-color: #e4eaef;
}
.i_basket_item:not(:first-child) {
	border-top-color: #d5ccbd;
}
.bx_ordercart .bx_ordercart_order_table_container .bx_ordercart_itemtitle {
	text-transform: uppercase;
	text-decoration: none;
	font-size: 14px;
	font-weight: bold;
}
.bx_ordercart .i_bs_delay {
	background: url(data:image/png;base64,);
}
.bx_ordercart .i_bs_delete {
	background: url(/local/templates/ilab_it_shop/ilab/img/svg/basket/delete.svg);
}
/* --------------------------------------------------------------------------- */
.basket_items_row, .i_basket_item {
	background: #eff2f4;
}
/* --------------------------------------------------------------------------- delayed_items */
.delayed_items_body_item:not(:first-child) {
	border-top-color: #d5ccbd;
}
.delayed_items_body_item {
	background-color: #eff2f4;
}
.delayed_items_head_row {
	background-color: #e4eaef;
}
/* -------------------------------------------------------------------------- bx_ordercart_order_table_body_row */
.bx_ordercart_order_table_body_row:not(:first-child) {
	border-top-color: #e4e4e4;
}
.i_ba_count {
	border-color: #eff2f4;
}
.custom {
	width: 150px!important;
}
.i_ba_minus,
.i_ba_plus {
	background-color: #fff;
}
.i_ba_minus {
	border-right: 1px solid #e7e7e7;
	border-radius: 5px 0 0 5px;
}
.i_ba_plus {
	border-left: 1px solid #e7e7e7;
	border-radius: 0 5px 5px 0;
}
.bx_sort_container span,
.i_ordercart_coupon span {
	color: #3e4e58;
}

@media screen and (min-width: 601px) {
	.bx_ordercart_order_table_head_row {
		background: #e4eaef;
	}
}
.bx_ordercart_order_pay .bx_ordercart_order_pay_right {
	background-color: #fff;
}