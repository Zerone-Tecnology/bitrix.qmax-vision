body {
	font-family: Arial, sans-serif;
	color: #3e4e58;
}
a, a:hover { color: #1f397d }
/* ---------------------------------------------------------------------------------------------------- Button */
.i_wrapper input[type='submit'],
.i_wrapper input[type='button'],
.i_but_ac,
.i_but_in { color: #FFF }
.i_wrapper input[type='submit'],
.i_wrapper input[type='button'],
.i_but_ac {
	border-color: #e30613;
	padding: 5px 30px;
	border-radius: 5px;
	text-decoration: none;
	background: #e30613;
	display: inline-block;
	font-size: 15px !important;
	text-transform: uppercase;
	font-weight: bold;
}
select,
input,
textarea {
	border-color: #e7e7e7;
}
.i_but_ac:hover {
	color: #fff;
}
.i_but_in {
	background: #606060;

}
.i_but_ou {
	background-color: #EFF2F4;
	border-color: #DFE0E8;
}
/* ---------------------------------------------------------------------------------------------------- Buy [Section] */
.i_buy_buttom, .i_buy_bought { color: #FFF }
.i_buy_buttom:hover,
.i_buy_bought:hover { color: #FFF }
.i_buy_buttom {
	border-color: #e30613;
	background: #e30613;
}
.i_buy_bought,
.icard_buy_bought {
	border-color: #1f397d;
	background-color: #FFF;
}
/* ---------------------------------------------------------------------------------------------------- Header */
.i_hback {
	border-color: #e7e7e7;
	color: #3e4e58;
	background: url(/local/templates/ilab_it_shop/ilab/img/svg/arrows/left_color.svg) 10px center no-repeat;
	background-size: 10px;
	font-size: 14px;
}
.i_hcontact {
	height: auto;
	font-size: 24px;
	color: #4e4e4f;
}
@media screen and (min-width: 971px) {
	.i_hcontact {
		top: 62px;
	}
	.i_hback {
		top: 62px;
	}
}
/* ---------------------------------------------------------------------------------------------------- Footer */
.i_footer { border-color: #e4e4e4; }
/* ---------------------------------------------------------------------------------------------------- Modal window */
.i_modal_tit {
	color: #FFF;

	border-color: #1f397d;

	background: #1f397d;
}
/* ---------------------------------------------------------------------------------------------------- empty basket */
.i_empty_basket { border-color: #e4e4e4 }
/* ---------------------------------------------------------------------------------------------------- SKU */
.i_sku { border-color: #c4c4c4 }
.i_sku li {
	background-color: #F5F5F5;
	border-color: #1f397d!important;
	box-shadow: 0 0 0 1px #FFF inset;
}
.i_sku li.i_sku_color { box-shadow: 0 0 0 2px #FFF inset }
/* ---------------------------------------------------------------------------------------------------- table */
.current_price {
	color: #3e4e58;
}
.custom_discount {
	font-size: 92%;
}
/* ---------------------------------------------------------------------------------------------------- Оформить заказ */
.place_order_div {
	border: 1px solid #f4f0eb;
	background-color: #eff2f4;
}
.bx_order_make .place_order_div {
	background-color: #fff;
	border: 1px solid #fff;
}
.i_or_title {
	border: 0 none;
	background-color: #e4eaef;
	color: #3e4e58;
	text-transform: uppercase;
}
.i_or_title::before {
	border-color: #1f397e;
	background: #1f397e url(data:image/png;base64,) no-repeat;
	background-position: center;
}
.i_or_title::after {
	border-left: 10px solid #1f397e;
	border-top: 10px solid transparent;
	border-bottom: 10px solid transparent;
	width: 0;
	height: 0;
	top: 50%;
	margin-top: -10px;
}

.order-checkout {
	border-color: transparent;
	background-color: #eff2f4;
}

.order-checkout h4 { color: #262626 }

.order-checkout .i_or_section { border-color: #C4C4C4 }
.i_label_div { border-color: #e4e4e4 }
.i_or_person_type .label:not(:first-child) { border-color: #e4e4e4 }
.i_or_person_type .label.i_or_checked {
	background-color: #1f397d;
	/*-webkit-box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.25) inset;
		moz-box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.25) inset;
			box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.25) inset;*/
}
.i_or_checked:before,
.i_or_checked:after {
	border-top-color: #1f397d;
}
.i_or_person_type .label.i_or_checked span {
	border-bottom: 0 none;
	color: #fff;
}
.i_or_person_type .label {
	border: 0 none;
	background-color: #fff
}
.i_or_person_type .label span {
	color: #3e4e58;
}

.i_or_payment .i_or_activ label .bx_logotype {
	border-color: #1f397d;
	background: #1f397d !important;
}
.i_or_payment .bx_block label .bx_logotype {
	background-color: #d6dee4;
	border-color: #d6dee4;
}
.i_or_payment .i_or_activ label .bx_description {
	border-color: #e4eaef;
	background: #e4eaef;
}
.i_or_payment .bx_block label .bx_description strong {
	color: #3e4e58;
	text-transform: uppercase;
}
.i_or_payment .bx_block.i_or_activ label .bx_description strong { color: #d40e15 }
.i_or_payment .bx_block.i_or_activ label .bx_description p { color: #3E3E3E }


.i_or_delivery .i_or_activ label .bx_description {
	border-color: #e4eaef;
	background: #e4eaef;
}
.i_or_delivery .i_or_activ label .bx_description:after, .i_or_payment .i_or_activ label .bx_description:after {
	border-right: 10px solid #e4eaef;
	border-top: 10px solid transparent;
	border-bottom: 10px solid transparent;
	width: 0;
	height: 0;
	background: none;
}
@media screen and (min-width: 481px) {
	.i_or_delivery .i_or_activ label .bx_description:after, .i_or_payment .i_or_activ label .bx_description:after {
		left: -10px;
	}
}
@media screen and (max-width: 480px) {
	.i_or_delivery .i_or_activ label .bx_description:after, .i_or_payment .i_or_activ label .bx_description:after {
		top: -15px;
		left: 40px;
	}
}
.i_or_delivery .i_or_activ label .bx_logotype {
	background-color: #FFF!important;
	-webkit-box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.60);
	moz-box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.60);
	box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.60);
}
.i_or_delivery .bx_block label .bx_logotype {
	background-color: #d6dee4;
	border-color: #d6dee4;
}
.i_or_delivery .i_or_activ label .bx_logotype {
	background-color: #1f397d !important;
	border-color: #1f397d;
	box-shadow: none;
}
.i_or_delivery .bx_block label .bx_description strong {
	color: #3e4e58;
	text-transform: uppercase;
}
.i_or_delivery .bx_block.i_or_activ label .bx_description strong { color: #d40e15 }

.i_checkout_inactive {
	text-shadow: none;
	cursor: default;
	border-color: transparent;

	background: #606060;
}
/* ---------------------------------------------------------------------------------------------------- Типовая страница */
p.i_typical_p {
	background-color: #EFF2F4;
	color: #6D6D6D;
}
.i_typical_ul li:before {
	background-color: #1f397d;
}
table.i_typical_table { border-color: #e4e4e4 }
table.i_typical_table tr:first-child {
	background: url(data:image/png;base64,) repeat-x 100% 100%;
	border-color: #e4e4e4;
}
table.i_typical_table tr:first-child th { border-color: #e4e4e4 }
table.i_typical_table tr:nth-child(n+3) {
	background-image: url(data:image/png;base64,);
}
/* ---------------------------------------------------------------------------------------------------- Условия заказа */
.i_c_i_system_expected { background-color: #1f397d }
.i_c_i_system_to_order { background-color: #e4dc1a }
.i_c_i_system_in_stock { background-color: #0f832c }
